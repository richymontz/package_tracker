<% flash.each do |type, message| %>
  <div class="col-md-12">
    <div class="alert alert-<%= alert_type(type) %>">
      <%= message %>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  </div>
<% end %>
